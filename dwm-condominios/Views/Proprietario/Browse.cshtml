@using App_Dominio.Repositories
@model PagedList<DWM.Models.Repositories.ProprietarioViewModel>
@{
    TransacaoRepository[] t = { new TransacaoRepository(), new TransacaoRepository() };
    t[0].url = "../Proprietario/Create";
    t[0].nomeCurto = "Incluir";
    t[0].referencia = "";

    t[1].url = "../Home/Default";
}
<!-- Datatables -->
<link href="../Content/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet">

<div class="x_panel">
    @Html.EditorFor(info => Model, "FORMULARIO", new { Brand = "Proprietários", Descricao = "", Transacao = t })
    <div class="x_content">
        <div id="div-Proprietario">
            @Html.Partial("List", Model)
        </div>
        <div class="altura-35">
            @Html.Partial("_defaultListButtons", new List<ButtonRepository> { new ButtonRepository() { linkText = "Incluir", actionName = "Create", controllerName = "Proprietario", buttonType = "link" } })
        </div>
    </div>
</div>

